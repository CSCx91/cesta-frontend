{
	"name": "cesta-frontend",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"classnames": "^2.2.6",
		"firebase": "^7.13.0",
		"node-sass": "^4.13.1",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-router-dom": "^5.1.2",
		"react-scripts": "3.3.1"
	},
	"scripts": {
		"build": "react-scripts build",
		"eject": "react-scripts eject",
		"eslint:check": " eslint . --cache --cache-location node_modules/.cache/eslint --ext .js,.jsx,.ts,.tsx",
		"eslint:fix": " eslint . --fix --cache --cache-location node_modules/.cache/eslint --ext .js,.jsx,.ts,.tsx",
		"precommit:check": "yarn prettier:check && yarn eslint:check",
		"precommit:fix": "yarn eslint:fix && yarn prettier:fix",
		"prettier:check": "prettier --check .prettierrc \"./src/**/*.{js,ts,tsx,jsx,json,md,scss}\"",
		"prettier:fix": "prettier --write .prettierrc \"./src/**/*.{js,ts,tsx,jsx,json,md,scss}\"",
		"start": "react-scripts start",
		"test": "react-scripts test"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"eslint": "^6.8.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.2",
		"prettier": "^1.19.1"
	}
}
